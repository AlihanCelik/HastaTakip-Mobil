<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_patientListFragment"
            app:destination="@id/patientListFragment" />
        <action
            android:id="@+id/action_homeFragment_to_teshisTedaviFragment"
            app:destination="@id/teshisTedaviFragment" />
        <action
            android:id="@+id/action_homeFragment_to_doktorListFragment"
            app:destination="@id/doktorListFragment" />
        <action
            android:id="@+id/action_homeFragment_to_randevuFragment"
            app:destination="@id/randevuFragment" />
    </fragment>
    <fragment
        android:id="@+id/patientListFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.patientList.PatientListFragment"
        android:label="fragment_patient_list"
        tools:layout="@layout/fragment_patient_list" >
        <action
            android:id="@+id/action_patientListFragment_to_patientInfoFragment"
            app:destination="@id/patientInfoFragment" >
            <argument
                android:name="hasta"
                app:argType="com.alihan.hastatakipuygulamas.data.model.Hasta"
                app:nullable="false" />
        </action>
        <action
            android:id="@+id/action_patientListFragment_to_addPatientFragment"
            app:destination="@id/addPatientFragment" >
            <argument
                android:name="hasta"
                app:argType="com.alihan.hastatakipuygulamas.data.model.Hasta"
                app:nullable="false" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/doktorListFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.doktorList.DoktorListFragment"
        android:label="fragment_doktor_list"
        tools:layout="@layout/fragment_doktor_list" >
        <action
            android:id="@+id/action_doktorListFragment_to_addDoktorFragment"
            app:destination="@id/addDoktorFragment" />
        <action
            android:id="@+id/action_doktorListFragment_to_doktorInfoFragment"
            app:destination="@id/doktorInfoFragment">
            <argument
                android:name="doktor"
                app:argType="com.alihan.hastatakipuygulamas.data.model.Doktor" />

        </action>
    </fragment>
    <fragment
        android:id="@+id/randevuFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.randevu.RandevuFragment"
        android:label="fragment_randevu"
        tools:layout="@layout/fragment_randevu" >
        <argument
            android:name="hasta"
            app:argType="com.alihan.hastatakipuygulamas.data.model.Hasta"
            app:nullable="true"/>
        <argument
            android:name="randevu"
            app:argType="com.alihan.hastatakipuygulamas.data.model.Randevu"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:id="@+id/teshisTedaviFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.TeshisTedaviFragment"
        android:label="fragment_teshis_tedavi"
        tools:layout="@layout/fragment_teshis_tedavi" />
    <fragment
        android:id="@+id/patientInfoFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.patientBilgi.PatientInfoFragment"
        android:label="fragment_patient_info"
        tools:layout="@layout/fragment_patient_info" >
        <argument
            android:name="hasta"
            app:argType="com.alihan.hastatakipuygulamas.data.model.Hasta" />
        <action
            android:id="@+id/action_patientInfoFragment_to_addPatientFragment"
            app:destination="@id/addPatientFragment" />
        <action
            android:id="@+id/action_patientInfoFragment_to_randevuListFragment"
            app:destination="@id/randevuListFragment">
            <argument
                android:name="hasta"
                app:argType="com.alihan.hastatakipuygulamas.data.model.Hasta"/>
        </action>
    </fragment>
    <fragment
        android:id="@+id/addPatientFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.addPatient.AddPatientFragment"
        android:label="AddPatientFragment">
        <argument
            android:name="hasta"
            app:argType="com.alihan.hastatakipuygulamas.data.model.Hasta"/>
    </fragment>
    <fragment
        android:id="@+id/addDoktorFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.addDoktor.AddDoktorFragment"
        android:label="AddDoktorFragment" >
        <argument
            android:name="doktor"
            app:argType="com.alihan.hastatakipuygulamas.data.model.Doktor"/>
    </fragment>
    <fragment
        android:id="@+id/doktorInfoFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.doktorInfo.DoktorInfoFragment"
        android:label="DoktorInfoFragment" >
        <argument
            android:name="doktor"
            app:argType="com.alihan.hastatakipuygulamas.data.model.Doktor" />
        <action
            android:id="@+id/action_doktorInfoFragment_to_addDoktorFragment"
            app:destination="@id/addDoktorFragment" >
            <argument
                android:name="doktor"
                app:argType="com.alihan.hastatakipuygulamas.data.model.Doktor"/>
        </action>
    </fragment>
    <fragment
        android:id="@+id/randevuListFragment"
        android:name="com.alihan.hastatakipuygulamas.presentation.randevuList.RandevuListFragment"
        android:label="RandevuListFragment" >
        <argument
            android:name="hasta"
            app:argType="com.alihan.hastatakipuygulamas.data.model.Hasta"/>
        <action
            android:id="@+id/action_randevuListFragment_to_randevuFragment"
            app:destination="@id/randevuFragment" >
            <argument
                android:name="hasta"
                app:argType="com.alihan.hastatakipuygulamas.data.model.Hasta"
                app:nullable="true"/>
            <argument
                android:name="randevu"
                app:argType="com.alihan.hastatakipuygulamas.data.model.Randevu"
                app:nullable="true"/>

        </action>
    </fragment>

</navigation>